{
    "extends": [
      "eslint-config-airbnb-base",
      "plugin:react/recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-unused-vars": [0],
      "no-underscore-dangle": [0],
      "no-param-reassign": [0],
      "camelcase": [0],
      "no-useless-constructor": [0],
      "class-methods-use-this": [0],
      "import/no-extraneous-dependencies": [0]
    }
    }
  